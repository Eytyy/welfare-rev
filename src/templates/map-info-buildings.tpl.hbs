<article class="project project--buildings">
  <div class="project__top">
    <header class="project__header">
      {{#if (ne this.BuildingNa " ")}}
        <h2 class="project__title">{{this.BuildingNa}}</h2>
      {{/if}}
      {{#if (ne this.eBuildingN " ")}}
        <h2 class="project__title">{{this.eBuildingN}}</h2>
      {{/if}}
      <div class="project__details project__details--main">
        {{#if (ne this.Founder " ")}}
          <div class="project__details__item project__founder">
            <div class="details__item__label">FOUNDER</div>
            <div class="details__item__value">{{this.Founder}}</div>
          </div>
        {{/if}}
        <div class="project__details__item project__knownRelations">
          <div class="details__item__label">KNOWN RELATION</div>
          <div class="details__item__value"></div>
        </div>
      </div>
    </header>
    <aside class="project__details">
      {{#if (ne this.otherName " ")}}
        <div class="project__details__item project__otherNames">
          <div class="details__item__label">OTHER NAMES</div>
          <div class="details__item__value">{{this.otherName}}</div>
        </div>
      {{/if}}
      {{#if (ne this.TimePeriod " ")}}
        <div class="project__details__item project__timePeriod">
          <div class="details__item__label">TIME PERIOD</div>
          <div class="details__item__value">{{this.TimePeriod}}</div>
        </div>
      {{/if}}
      {{#if (ne this.BuildingIm " ")}}
        <div class="project__details__item project_buildingImportance">
          <div class="details__item__label">BUILDING IMPORTANNCE</div>
          <div class="details__item__value">{{this.BuildingIm}}</div>
        </div>
      {{/if}}
      {{#if (ne this.BuildingUs " ")}}
        <div class="project__details__item project__usage">
          <div class="details__item__label">USGAE</div>
          <div class="details__item__value">{{this.BuildingUs}}</div>
        </div>
      {{/if}}
      {{#if (ne this.FounderTyp " ")}}
        <div class="project__details__item project__founderType">
          <div class="details__item__label">FOUNDER TYPE</div>
          <div class="details__item__value">{{this.FounderTyp}}</div>
        </div>
      {{/if}}
      {{#if (ne this.Reference " ")}}
        <div class="project__details__item project__reference">
          <div class="details__item__label">REFERENCES</div>
          <div class="details__item__value">{{this.Reference}}</div>
        </div>
      {{/if}}
    </aside>
    <div class="project__details--buildings-extras">
      {{#if (ne this.BuildingTy " ")}}
        <div class="project__details__item project__buildingType">
          <div class="details__item__label">BUILDING TYPE</div>
          <div class="details__item__value">{{this.BuildingTy}}</div>
        </div>
      {{/if}}
      {{#if (ne this.BuildingDa " ")}}
        <div class="project__details__item project__buildingDate">
          <div class="details__item__label">BUILDING Date</div>
          <div class="details__item__value">{{this.BuildingDa}}</div>
        </div>
      {{/if}}
      {{#if (ne this.RestoredIn " ")}}
        <div class="project__details__item project__restoredIn">
          <div class="details__item__label">Restored In</div>
          <div class="details__item__value">{{this.RestoredIn}}</div>
        </div>
      {{/if}}
      {{#if (ne this.originalUs " ")}}
        <div class="project__details__item project__originalUse">
          <div class="details__item__label">Original Use</div>
          <div class="details__item__value">{{this.originalUs}}</div>
        </div>
      {{/if}}
      {{#if (ne this.CurrentUse " ")}}
        <div class="project__details__item project__currentUse">
          <div class="details__item__label">Current Use</div>
          <div class="details__item__value">{{this.CurrentUse}}</div>
        </div>
      {{/if}}
    </div>
  </div>
</article>
